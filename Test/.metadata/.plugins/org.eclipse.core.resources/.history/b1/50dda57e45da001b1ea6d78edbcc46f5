package helloworld;

public class Test3 {
public static void main(String[]args) {
	int[] a={1,5,3,2,7,9};

	for(int i=1;i<a.length;i++) {
		for(int j=i-1;j<a.length;j++) {
			
			while((a[j]>a[j+1])&&(a[j]==0)) {
				a[j+1]=a[j];
				j--;
			}
		}
	}
	for(int z=0;z<a.length;z++) {
		System.out.print(a[z]+" ");
	}
}
}
